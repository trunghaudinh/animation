<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        android:id="@+id/transitionFirst"
        motion:constraintSetEnd="@+id/mid"
        motion:constraintSetStart="@+id/start1" />
    <ConstraintSet android:id="@+id/start1">
        <Constraint
            android:layout_height="140dp"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView3"
            android:layout_width="140dp"
            android:id="@+id/cardView"
            motion:layout_constraintTop_toTopOf="@+id/imageView3"
            motion:layout_constraintEnd_toEndOf="@+id/imageView3"
            motion:layout_constraintStart_toStartOf="@+id/imageView3" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/mid"></ConstraintSet>
    <ConstraintSet android:id="@+id/end1"></ConstraintSet>


</MotionScene>